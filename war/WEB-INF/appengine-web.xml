<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>musicbymap</application>
  <version>1</version>

  <!--
    By default, App Engine sends requests serially to a given web server.
    To allow App Engine to send multiple requests in parallel specify:
-->
  <threadsafe>true</threadsafe>

 
    <static-files>
        <include path="/favicon.ico" expiration="5d" />
        <include path="/js/**.js" />
        <include path="/img/**.jpg" />
        <include path="/css/**.css" />
        <include path="/index.html" />
    </static-files>
    
  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/> 
  </system-properties>
 
 <warmup-requests-enabled>true</warmup-requests-enabled>
</appengine-web-app>
